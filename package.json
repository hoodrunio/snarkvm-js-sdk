{
  "name": "aleo-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for SnarkVM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "@noble/hashes": "^1.3.2",
    "@scure/base": "^1.1.3",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@types/bn.js": "^5.1.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.51.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.0"
  },
  "keywords": [
    "aleo",
    "blockchain",
    "cryptography",
    "zero-knowledge",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
